{
  "version": 1,
  "domain": "lobste.rs",
  "display_name": "Lobsters",

  "description": "Computing-focused community centered around link aggregation and discussion",

  "brand_color": "#AC130D",

  "logo": {
    "svg_black": "https://lobste.rs/small-black-logo.svg",
    "svg_full": "https://lobste.rs/full-color.logo.svg"
  },

  "username": {
    "re": "^[A-Za-z0-9][A-Za-z0-9_-]{0,24}$",
    "min": 1,
    "max": 25
  },

  "prefill_url": "https://lobste.rs/settings/keybase_auth?kb_username=%{kb_username}&kb_signature=%{sig_hash}&kb_ua=%{kb_ua}&username=%{username}",

  "profile_url": "https://lobste.rs/u/%{username}",

  "check_url": "https://lobste.rs/u/%{username}.json",
  "check_path": ["keybase_signatures"],
  "avatar_path": ["avatar_url"],

  "contact": ["xxx@lobste.rs"]
}
